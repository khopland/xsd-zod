{
  "name": "xsd-zod",
  "type": "module",
  "version": "0.1.0",
  "description": "Generate Zod validators and TypeScript types from XSD schemas",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/khopland/xsd-zod#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khopland/xsd-zod.git"
  },
  "bugs": {
    "url": "https://github.com/khopland/xsd-zod/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "bin": {
