    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "tsdown": "0.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
